{
  "manifest_version": 3,
  "name": "Censorship",
  "version": "1.0",
  "description": "Censors specific words on any website.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}
